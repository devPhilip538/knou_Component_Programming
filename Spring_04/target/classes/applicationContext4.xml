<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	<context:annotation-config/>
	<bean id="profile" class="com.springbook.biz.annotation.resource.Profile">
	</bean>
	<bean id="student1" class="com.springbook.biz.annotation.resource.Student">
		<property name="name" value="zara"></property>
		<property name="age" value="11"></property>
	</bean>
	<bean id="student2" class="com.springbook.biz.annotation.resource.Student">
		<property name="name" value="nuha"></property>
		<property name="age" value="2"></property>
	</bean>
</beans>
